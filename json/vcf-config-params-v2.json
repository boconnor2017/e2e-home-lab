# ******************* E X T E R N A L    I N F R A S T R U C T U R E    C O M P O N E N T S *******************
# NTP server to configure for the deployed products. This should match the root Active Directory server if not deploying with an external AD.
ntp-server@address=pool.ntp.org
remote-site-ntp-server@address=

# DNS Zones
ldapADIdentitySource.domainName=e2e.local
ldapADIdentitySource.subDomainPrefix=e2e.local

# Site Name to be used for vSphere Single-Sign-on in mgmt and comp
sso-site-name@value=vcf.local

# Default Portgroup for VM Deployments
default-network-name=VM Network


# ******************* S D D C    M A N A G E R  *******************
# Default credentials for SDDC Manager 
sddc-manager-root-credentials=VMware1!
sddc-manager-superuser-credentials=VMware1!
sddc-manager-restapi-user-credentials=VMware1!
#REF!
#REF!

# SDDC Manager Appliance
sddcManagerIp.Address=172.16.10.6
sddcManagerIp.netmask=255.255.255.0
sddcManager-deployment-vmname=e2e-sddc-manager
sddcManager-HostPoolName=e2e01-networkpool


# ******************* S D D C    I N F R A S T R U C T U R E *******************
# Default credentials for all ESXi servers, all installations must have the same user name and password.
esxi.username=root
esxi.password=

# vCenter OS credentials (for the root user)
vcenter-root-credentials@password=
# vCenter product credentials (for the Administrator@vsphere.local user)
vcenter-admin-credentials@password=

# NSX product credentials (for the admin user)
nsx-admin-credentials@password=
nsx-edge-cli-credentials@password=nsx@VMware1!
nsx-cli-privilege-credentials@password=

# Physical NIC name which will be used when the hosts are attached to vDS
physical-nic-dedicated-to-mgmt-dvs@value=vmnic1
# VM Kernel Adaptors migrated to vDS
vm-kernel-adapter-management-name@value=vmk0
# Name of vSphere Standard Switch to be migrated. The other standard switches are left intact.
#If no value is provided, then a single standard switch is expected on each host
management-hosts-vss-name@value=vSwitch0
management-hosts-vmnic=vmnic0

# VSAN datastores names
management-vsan-datastore-name@value=e2e01-m01-vsan
# Enable vSAN Deduplication and Compression
enableVsanDeduplication=false

# ESXi Security Thumbprints
skipThumbprintValidation=true
# SSH Thumbprints
esxi.mgmt-1.sshThumbprint=
esxi.mgmt-2.sshThumbprint=
esxi.mgmt-3.sshThumbprint=
esxi.mgmt-4.sshThumbprint=
# SSL Thumbprints
esxi.mgmt-1.sslThumbprint=
esxi.mgmt-2.sslThumbprint=
esxi.mgmt-3.sslThumbprint=
esxi.mgmt-4.sslThumbprint=

# Hosts needed for the management cluster, this is where we deploy all the solutions. Up to 8 hosts, 2 is the minimum.
esxi.mgmt-1.address=172.16.10.10
esxi.mgmt-1.hostname=nesxi01.e2e.local
esxi.mgmt-2.address=172.16.10.11
esxi.mgmt-2.hostname=nesxi02.e2e.local
esxi.mgmt-3.address=172.16.10.12
esxi.mgmt-3.hostname=nesxi03.e2e.local
esxi.mgmt-4.address=172.16.10.13
esxi.mgmt-4.hostname=nesxi04.e2e.local
esxi.mgmt-5.address=
esxi.mgmt-5.hostname=
esxi.mgmt-6.address=
esxi.mgmt-6.hostname=
esxi.mgmt-7.address=
esxi.mgmt-7.hostname=
esxi.mgmt-8.address=
esxi.mgmt-8.hostname=
#Network settings and static IPs for vMotion VMKernel DV portgroups
mgmtVmotionNetwork.cidrNotation=172.16.11.0/24
mgmtVmotionNetwork.gateway=172.16.11.1
#Network settings and static IPs for VSAN VMKernel DV portgroups
mgmtVsanNetwork.cidrNotation=172.16.12.0/24
mgmtVsanNetwork.gateway=172.16.12.1

# VTEP network for Management NSX
vtepManagementNetwork.gateway=
vtepManagementNetwork.primaryDns=172.16.0.9
vtepManagementNetwork.secondaryDns=8.8.8.8
vtepManagementNetwork.searchDomain=e2e.local
vtepManagementNetwork.cidrNotation=

# IP pool on the Management VTEP network
nsx-management-vtep-ip-pool.startAddress=
nsx-management-vtep-ip-pool.endAddress=

# Segment ID ranges (Management NSX)
mgmt-nsx-segment-id-range@rangeStart=5000
mgmt-nsx-segment-id-range@rangeEnd=5200

# Multicast address ranges (Management NSX)
nsx-mgmt-multicast-address-range@rangeStartIp=239.1.0.0
nsx-mgmt-multicast-address-range@rangeEndIp=239.1.255.255

# Datacenter/cluster names (if not specified, the default values will be used)
mgmt-datacenter-name=e2e01-m01-dc
management-cluster-name=e2e01-m01-mgmt01

#EVC cluster mode. If not specified, EVC will be disabled on all clusters. Allowed values are:
# - "intel-merom", "intel-penryn", "intel-nehalem", "intel-westmere", "intel-sandybridge", "intel-ivybridge", "intel-haswell",
# - "amd-rev-e", "amd-rev-f", "amd-greyhound-no3dnow", "amd-greyhound", "amd-bulldozer", "amd-piledriver"
evc-mode-management-cluster@value=

# Host Profile Names (2 POD Only)
management-host-profile-name@value=e2e01-w01-user-edge

# Folder Names Mgmt Cluster - Automatically formulated in XLS using sso-site-name@value= + static values
mgmt-vm-folder-name@value=vcf.local-m01fd-mgmt
nsx-vm-folder-name@value=vcf.local-m01fd-nsx
availability-vm-folder-name@value=vcf.local-m01fd-bcdr
vra-vm-folder-name@value=vcf.local-m01fd-vra
vra-ias-vm-folder-name@value=vcf.local-m01fd-vraias
vrops-vm-folder-name@value=vcf.local-m01fd-vrops
vrops-rc-vm-folder-name@value=vcf.local-m01fd-vropsrc
vrli-vm-folder-name@value=vcf.local-m01fd-vrli

# vSphere Resource Pools
vsphere-resource-pools[1]=e2e01-w01-sddc-mgmt
vsphere-resource-pools[2]=e2e01-w01-sddc-edge
vsphere-resource-pools[3]=e2e01-w01-user-edge
vsphere-resource-pools[4]=e2e01-w01-user-vm

# Distributed Virtual Switch Names and Portgroups
vds-management-initial-configuration@dvSwitchName=e2e01-m01-vds
# Management vDS Portgroup Naming
vds-management-initial-configuration@dvPortGroups[1].name=SDDC-DPortGroup-Mgmt
vds-management-initial-configuration@dvPortGroups[2].name=SDDC-DPortGroup-VSAN 
vds-management-initial-configuration@dvPortGroups[3].name=SDDC-DPortGroup-vMotion

# Management network settings
managementNetwork.gateway=172.16.10.1
managementNetwork.primaryDns=172.16.0.9
managementNetwork.secondaryDns=8.8.8.8
managementNetwork.searchDomain=e2e.local
managementNetwork.cidrNotation=172.16.10.0/24

# IP pools on the main network
nsx-controllers-ip-pool.startAddress=172.16.10.118
nsx-controllers-ip-pool.endAddress=172.16.10.120

# NSX Controller Names
nsxControllers.mgmt=vcf.localm01nsxc0%s

# Network VLAN ID Configuration
# Management Portgroup - VLAN Settings
vlan-mgmt-management.vlanId=1611
# vMotion Portgroup  - VLAN Settings
vlan-management-vmotion.vlanId=1612
# VSAN Portgroup  - VLAN Settings
vlan-management-vsan.vlanId=1613
# VXLAN Portgroup - VLAN Settings
vlan-management-vxlan.vlanId=1614

# Network IP Inclusion Ranges
inclusion-range-start-vmotion01=172.16.11.10
inclusion-range-end-vmotion01=172.16.11.100
inclusion-range-start-vmotion02=
inclusion-range-end-vmotion02=
inclusion-ips-vmotion=
