inclusion-range-end-vsan01=172.16.12.100
inclusion-range-start-vsan02=
inclusion-range-end-vsan02=
inclusion-ips-vsan=

# VLAN MTU configuration
# Management Portgroup - MTU Settings
vlan-mgmt-management-mtu@mtu=9000
# Vmotion MTU
vlan-management-vmotion-mtu@mtu=9000
# vSAN MTU
vlan-management-vsan-mtu@mtu=9000
# VDS MTU - Used for VDS, VTEP, ESGs
